{
  "version": "0.2.0",
  "configurations": [{
    "name": "dev-build",
    "type": "node",
    "program": "/usr/local/bin/gulp",
    "request": "launch",
    "args": ["start"],
    "cwd": "${workspaceRoot}",
    "runtimeExecutable": null,
    "env": {
      "NODE_ENV": "development"
    }
  }, {
    "name": "prod-build",
    "type": "node",
    "program": "/usr/local/bin/gulp",
    "request": "launch",
    "args": ["start"],
    "cwd": "${workspaceRoot}",
    "runtimeExecutable": null,
    "env": {
      "NODE_ENV": "production"
    }
  }]
}